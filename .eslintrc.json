{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": ["off"],
    "arrow-body-style": ["off"],
    "prefer-const": ["off"],
    "no-underscore-dangle": ["off"],
    "consistent-return": ["off"],
    "func-name": ["off"],
    "no-shadow": ["off"]
  }
}
